        <activiti:formProperty id="oplata_1" name="Платіжний документ (квитанція) про оплату за внесення змін; можна завантажити електронну квітанцію, сформовану в інтернет-банку" type="file"></activiti:formProperty>
        <activiti:formProperty id="oplata_2" name="Платіжний документ (квитанція) про оплату за друк інформації" type="file"></activiti:formProperty>
        <activiti:formProperty id="no_debts" name="довідка відповідного органу доходів і зборів про відсутність заборгованості із сплати податків, зборів" type="file"></activiti:formProperty>
        <activiti:formProperty id="pension" name="довідка відповідного органу Пенсійного фонду України про відсутність заборгованості із сплати єдиного внеску" type="file"></activiti:formProperty>
        <activiti:formProperty id="archive" name="довідка архівної установи про прийняття документів, які відповідно до закону підлягають довгостроковому зберіганню" type="file"></activiti:formProperty>
        <activiti:formProperty id="text" name="Зверніть увагу:" type="label" default="Замовляючи послугу Ви надаєте згоду на обробку Ваших персональних даних та підтверджуєте їх актуальність."></activiti:formProperty>
        <activiti:formProperty id="rName" name="номер району" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="markers30" name="маркер м.Вільногірськ" type="markers" default="{&quot;motion&quot;:{&quot;ValuesFieldsOnCondition_30&quot;:{&quot;aField_ID&quot;:[&quot;rName&quot;], &quot;asID_Field_sValue&quot;:[&quot;25&quot;],&quot;asID_Field&quot;:{&quot;sReason&quot;:&quot;area&quot;},&quot;sCondition&quot;:&quot;[sReason]=='vilnogirsk'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers2" name="якщо саморозпуск" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_2&quot;:{&quot;aField_ID&quot;:[&quot;decision_dissolution&quot;,&quot;svidotstvo&quot;,&quot;statute&quot;,&quot;info_0&quot;,&quot;objReestr&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;decision_0&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'dissolution'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers3" name="якщо реорганізація" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_3&quot;:{&quot;aField_ID&quot;:[&quot;info_3&quot;,&quot;decision_join&quot;,&quot;svidotstvo_1&quot;,&quot;statute_1&quot;,&quot;info_1&quot;,&quot;objReestr_1&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;decision_0&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'reorganization'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers4" name="якщо реорганізація у статуті" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_4&quot;:{&quot;aField_ID&quot;:[&quot;doc_change&quot;,&quot;protocol&quot;,&quot;info_2&quot;,&quot;objReestr_2&quot;,&quot;statute_2&quot;,&quot;info_4&quot;,&quot;zgoda_join&quot;,&quot;svidotstvo_2&quot;,&quot;statute_3&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'statute'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers5" name="якщо реорганізація та зміни у найменуванні" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_5&quot;:{&quot;aField_ID&quot;:[&quot;info&quot;,&quot;label&quot;,&quot;oplata_1&quot;,&quot;oplata_2&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;сhanged&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'name'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers6" name="якщо реорганізація та зміни у найменуванні і статуті" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_6&quot;:{&quot;aField_ID&quot;:[&quot;doc_change&quot;,&quot;protocol&quot;,&quot;info_2&quot;,&quot;objReestr_2&quot;,&quot;statute_2&quot;,&quot;info_4&quot;,&quot;zgoda_join&quot;,&quot;svidotstvo_2&quot;,&quot;statute_3&quot;,&quot;info&quot;,&quot;label&quot;,&quot;oplata_1&quot;,&quot;oplata_2&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;сhanged&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'name_statute'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers7" name="якщо припинення діяльності" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_7&quot;:{&quot;aField_ID&quot;:[&quot;no_debts&quot;,&quot;pension&quot;,&quot;archive&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;сhanged&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'the_end'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="form_signed" name=" " type="file"></activiti:formProperty>
      </extensionElements>
    </startEvent>
