        <activiti:formProperty id="objReestr" name="Заповнена реєстраційна картка на проведення державної реєстрації припинення юридичної особи" type="file"></activiti:formProperty>
        <activiti:formProperty id="info_3" name=" " type="label" default="Документи громадського об’єднання, &lt;b&gt;яке приєднується&lt;/b&gt;"></activiti:formProperty>
        <activiti:formProperty id="decision_join" name="Рішення, про припинення діяльності з приєднанням до іншого об’єднання" type="file"></activiti:formProperty>
        <activiti:formProperty id="svidotstvo_1" name="Примірник свідоцтва про реєстрацію громадського об’єднання" type="file"></activiti:formProperty>
        <activiti:formProperty id="statute_1" name="Примірник статуту" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="info_1" name="Заповніть реєстраційну картку (форма 8)" type="label" default="Завантажте &lt;a href=&quot;https://drive.google.com/open?id=0B6mOkUg9oq1zMUp0b1gycUNuY1E&quot; target=&quot;_blank&quot;&gt;бланк&lt;/a&gt;, заповніть, накладіть на нього електронно-цифровий підпис та додайте у наступному полі."></activiti:formProperty>
        <activiti:formProperty id="objReestr_1" name="Заповнена реєстраційна картка на проведення державної реєстрації припинення юридичної особи" type="file"></activiti:formProperty>
        <activiti:formProperty id="doc_change" name="Копія рішення вищого органу управління громадського об’єднання про внесення змін до статуту; файл в форматі PDF повинен мати ЕЦП" type="file"></activiti:formProperty>
        <activiti:formProperty id="protocol" name="Примірник  протоколу засідання вищого керівного органу громадського об’єднання; файл в форматі PDF повинен мати ЕЦП" type="file"></activiti:formProperty>
        <activiti:formProperty id="info_2" name="Заповніть реєстраційну картку (форма 3)" type="label" default="Завантажте &lt;a href=&quot;https://drive.google.com/open?id=0B6mOkUg9oq1zMUp0b1gycUNuY1E&quot; target=&quot;_blank&quot;&gt;бланк&lt;/a&gt;, заповніть, накладіть на нього електронно-цифровий підпис та додайте у наступному полі."></activiti:formProperty>
