        <activiti:formProperty id="zgoda_join" name="Згода на приєднання іншої громадської організації" type="file"></activiti:formProperty>
        <activiti:formProperty id="svidotstvo_2" name="Примірник свідоцтва громадського об’єднання, до якого приєднуються" type="file"></activiti:formProperty>
        <activiti:formProperty id="statute_3" name="Примірник статуту, до якого приєднуються" type="file"></activiti:formProperty>
