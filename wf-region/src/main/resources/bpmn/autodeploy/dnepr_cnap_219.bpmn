        <activiti:formProperty id="comment_1" name="У разі відхилення звернення внесіть, будь ласка, причину" type="textArea"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression>${bankId_scan_passport},${decision_dissolution},${svidotstvo},${statute},${objReestr},${decision_join},${svidotstvo_1},${statute_1},${objReestr_1},${doc_change},${protocol},${objReestr_2},${statute_2},${zgoda_join},${svidotstvo_2},${statute_3},${oplata_1},${oplata_2},${no_debts},${pension},${archive},${form_signed}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
