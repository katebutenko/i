        <activiti:formProperty id="objName" name="Найменування громадського об’єднання" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="decision_dissolution" name="Рішення про саморозпуск громадського об’єднання" type="file"></activiti:formProperty>
        <activiti:formProperty id="svidotstvo" name="Примірник свідоцтва про реєстрацію громадського об’єднання" type="file"></activiti:formProperty>
        <activiti:formProperty id="statute" name="Примірник статуту" type="file"></activiti:formProperty>
