        <activiti:formProperty id="decision_0" name="зазначене рішення громадського об’єднання" type="enum" writable="false">
          <activiti:value id="dissolution" name="саморозпуск"></activiti:value>
          <activiti:value id="reorganization" name="реорганізація"></activiti:value>
          <activiti:value id="the_end" name="припинення діяльності"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="сhanged" name="Чи відбуваються зміни у найменуванні чи статуті громадського обїєднання до якого приєднуються?" type="enum" writable="false">
          <activiti:value id="empty" name=" "></activiti:value>
          <activiti:value id="no_changes" name="ні"></activiti:value>
          <activiti:value id="name" name="так - зміна найменування"></activiti:value>
          <activiti:value id="statute" name="так - змінюється статут"></activiti:value>
