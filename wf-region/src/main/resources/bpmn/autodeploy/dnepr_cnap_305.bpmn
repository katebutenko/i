          <activiti:expression>noreply@igov.org.ua</activiti:expression>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>crow79@online.ua</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>Результат опрацювання звернення № ${RNo} від ${RDate}</activiti:expression>
