        <activiti:formProperty id="obect" name="Укажіть номер, дату та назву рішення міської ради" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="micce" name="Місцезнаходження (адреса) об’єкта, на який видається документ" type="string" required="true"></activiti:formProperty>
