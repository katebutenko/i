    <serviceTask id="mailtask1" name="e-mail о реєстрації запиту" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Копія рішення міської ради про передачу в оренду земельних ділянок державної або комунальної власності]]></activiti:string>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string><![CDATA[UTF-8]]></activiti:string>
        </activiti:field>
