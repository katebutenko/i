quot;:{&quot;aField_ID&quot;:[&quot;dms_center&quot;,&quot;nID_Department_visitDate&quot;],&quot;asID_Field_sValue&quot;:[&quot;7&quot;,&quot;21&quot;],&quot;asID_Field&quot;:{&quot;sDms&quot;:&quot;area&quot;},&quot;sCondition&quot;:&quot;[sDms] == 'dnepr7'&quot;},&quot;ValuesFieldsOnCondition_8&quot;:{&quot;aField_ID&quot;:[&quot;dms_center&quot;,&quot;nID_Department_visitDate&quot;],&quot;asID_Field_sValue&quot;:[&quot;8&quot;,&quot;22&quot;],&quot;asID_Field&quot;:{&quot;sDms&quot;:&quot;area&quot;},&quot;sCondition&quot;:&quot;[sDms] == 'dnepr8'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="dms_center" name="dms_center" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="date_of_visit" name="дата візиту" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="notification_day" name="notification_day" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="sCancelInfo" name="sCancelInfo" type="invisible" default="Заявка актуальна"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <userTask id="usertask2" name="обробка дмс" activiti:candidateGroups="dnepr_dms_passport_${area}">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькові" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="have_passport" name="У Вас на даний момент є закордонний паспорт?" type="enum" writable="false">
          <activiti:value id="attr1_yes" name="так, хочу подовжити"></activiti:value>
          <activiti:value id="attr2_no" name="ні, буду отримувати перший раз"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="urgent" name="Паспорт треба виготовити терміново?; (протягом 10 днів за додаткову плату - ціна у вкладці &quot;Як це працює&quot;)" type="enum" readable="false" writable="false">
          <activiti:value id="attr1_no" name="ні"></activiti:value>
          <activiti:value id="attr2_yes" name="так"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="biometrical" name="Ви бажаєте замовити біометричний паспорт?" type="enum" writable="false">
          <activiti:value id="attr1_no" name="ні"></activiti:value>
          <activiti:value id="attr2_yes" name="так"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="phone" name="Ваш телефон" type="long" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="Ваша електронна пошта" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="visitDate" name="В який день Ви бажаєте звернутися до міграційної служби?" type="queueData" writable="false"></activiti:formProperty>
