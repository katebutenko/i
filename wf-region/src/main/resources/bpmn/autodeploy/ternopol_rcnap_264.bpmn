        <activiti:formProperty id="markers4" name="маркер Вклеювання фотографії у посвідчення дитини з багатодітеої сім'ї" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_4&quot;:{&quot;aField_ID&quot;:[&quot;pib_father&quot;,&quot;pib_mother&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;purpose&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'add_foto_14_years'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers5" name="маркер Продовження строку дії посвідчень багатодітної сім'ї у зв'язку з народженням дитини" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_5&quot;:{&quot;aField_ID&quot;:[&quot;passport_children&quot;, &quot;passport_father&quot;,&quot;passport_mother&quot;,&quot;family&quot;,&quot;together&quot;,&quot;student&quot;,&quot;pib_father&quot;,&quot;pib_mother&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;purpose&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'extension_new_child'&quot;}}}"></activiti:formProperty>
