        <activiti:formProperty id="coordination" name="Нотаріально засвідчене погодження землекористувача;(у разі вилучення земельної ділянки, що перебуває у користванні інших осіб)." type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file1" name="Копія Свідоцтва  про державну реєстрацію ОСББ, ЖБК, ОК (витяг)" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file2" name="Копія довідки про внесення до Єдиного державного реєстру підприємств та організацій України" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file3" name="Копія статуту ОСББ, ЖБК, ОК" type="file" default=" "></activiti:formProperty>
