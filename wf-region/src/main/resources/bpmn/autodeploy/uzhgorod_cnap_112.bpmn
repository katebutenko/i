          <activiti:value id="attr2" name="Юридична особа або фізична особа - підприємець"></activiti:value>
          <activiti:value id="attr3" name="ОСББ, ЖБК, ОК"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="bankId_scan_passport" name="Копія паспорту" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="scan_inn" name="Копія довідки про присвоєння ідентифікаційного номера" type="file" default=" "></activiti:formProperty>
