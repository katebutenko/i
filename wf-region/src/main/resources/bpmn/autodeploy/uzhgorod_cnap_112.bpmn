        <activiti:formProperty id="file5" name="Копія акту приймання-передачі житлового комплексу на баланс ОСББ" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file6" name="Проекти розподілу території кварталу або мікрорайону з відображенням місця знаходження земельної ділянки з визначенням меж та площі земельної ділянки прибудинкової території, нанесеними червоними лініями, розроблені проектними організаціями, що мають відповідну ліцензію на виконання даного виду робіт" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file7" name="План благоустрою, розроблений на підставі технічних умов і погоджений Департаментом міського господарства міської ради, управлінням містобудування та архітектури;(за наявності)" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file8" name="Засвідчена нотаріально письмова згода землекористувача (землевласника) на вилучення земельної ділянки (її частини) із зазначенням розмірів, передбачених для вилучення та умов її вилучення;(в разі, якщо земельна ділянка потребує такого вилучення)" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file9" name="Копії документів, що посвідчують право користування земельною ділянкою;(за наявності)" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file10" name="Копії рішень уповноважених органів про передачу (надання) земельної ділянки в користування;(за наявності)" type="file" default=" "></activiti:formProperty>
        <activiti:formProperty id="file11" name="Рішення (протокол) Загальних зборів ОСББ, ЖБК, ОК про необхідність оформлення права власності або користування земельною ділянкою" type="file" default=" "></activiti:formProperty>     
        <activiti:formProperty id="notice" name="Зверніть увагу" type="label" default="Замовляючи послугу Ви надаєте згоду на обробку Ваших персональних даних та підтверджуєте їх актуальність."></activiti:formProperty>
        <activiti:formProperty id="marker1" name="Фізична особа" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_1&quot;:{&quot;aField_ID&quot;:[&quot;bankId_scan_passport&quot;,&quot;scan_inn&quot;,&quot;graphics&quot;,&quot;right&quot;,&quot;coordination&quot;],&quot;asID_Field&quot;:{&quot;sReason&quot;:&quot;subject_of_appeal&quot;},&quot;sCondition&quot;:&quot;[sReason] == 'attr1'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker2" name="Юридична особа або фізична особа - підприємець" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_2&quot;:{&quot;aField_ID&quot;:[&quot;copy_vytyag&quot;,&quot;graphics&quot;,&quot;right&quot;,&quot;coordination&quot;],&quot;asID_Field&quot;:{&quot;sReason1&quot;:&quot;subject_of_appeal&quot;},&quot;sCondition&quot;:&quot;[sReason1] == 'attr2'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="marker3" name="ОСББ, ЖБК, ОК" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_3&quot;:{&quot;aField_ID&quot;:[&quot;file1&quot;,&quot;file2&quot;,&quot;file3&quot;,&quot;file4&quot;,&quot;file5&quot;,&quot;file6&quot;,&quot;file7&quot;,&quot;file8&quot;,&quot;file9&quot;,&quot;file10&quot;,&quot;file11&quot;],&quot;asID_Field&quot;:{&quot;sReason2&quot;:&quot;subject_of_appeal&quot;},&quot;sCondition&quot;:&quot;[sReason2] == 'attr3'&quot;}}}"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask1" name="Завантаження файлів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
